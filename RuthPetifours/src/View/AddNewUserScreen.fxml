<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.AddNewUserController">
   <children>
      <ImageView fitHeight="419.0" fitWidth="547.0" pickOnBounds="true" preserveRatio="true" />
      <TextField fx:id="name" layoutX="190.0" layoutY="14.0" promptText="Insert Name" />
      <Button fx:id="Save" layoutX="142.0" layoutY="388.0" mnemonicParsing="false" onAction="#addNewUserToDB" style="-fx-background-color: green;" text="Save" />
      <Button fx:id="cancel" layoutX="357.0" layoutY="388.0" mnemonicParsing="false" onAction="#cancelAndGoBack" style="-fx-background-color: red;" text="Cancel" />
      <TextField fx:id="password" layoutX="190.0" layoutY="55.0" promptText="Insert Password" />
      <TextField fx:id="question1" layoutX="149.0" layoutY="98.0" prefHeight="27.0" prefWidth="249.0" promptText="Insert 1st backup question" />
      <TextField fx:id="question2" layoutX="149.0" layoutY="183.0" prefHeight="27.0" prefWidth="249.0" promptText="Insert 2nd backup question" />
      <TextField fx:id="answer1" layoutX="149.0" layoutY="140.0" prefHeight="27.0" prefWidth="249.0" promptText="Insert 1st question answer" />
      <TextField fx:id="answer2" layoutX="149.0" layoutY="225.0" prefHeight="27.0" prefWidth="249.0" promptText="Insert 2nd question answer" />
      <ComboBox fx:id="jobRole" layoutX="190.0" layoutY="285.0" prefHeight="27.0" prefWidth="167.0" promptText="What is Your role?" />
   </children>
</AnchorPane>
